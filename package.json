    "request": "^2.88.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/mime-types": "^2.1.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.2",
    "@types/request": "^2.48.1",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
